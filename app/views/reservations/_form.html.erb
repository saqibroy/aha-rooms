<%= simple_form_for(@reservation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :no_of_rooms %>
    <%= f.input :total_price %>
    <%= f.input :check_in_date %>
    <%= f.input :check_out_date %>
    <%= f.input :room_id %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
